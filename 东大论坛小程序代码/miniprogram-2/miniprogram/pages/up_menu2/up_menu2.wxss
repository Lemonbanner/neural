/* pages/bottom_menu/bottom_menu.wxss */
.bottomrow{
  background-color: white;
  width: 100%;
  height: 8%;
  top: 0rpx;
  position: fixed;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.bottomtab{
  width: 250rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.bottomImg{
    margin-top: 10rpx;
    width: 130rpx;
    height: 110rpx;
    border-radius: 10rpx;
}
.bottomtabText{
    font-size: 35rpx;
    color: #979797;
}